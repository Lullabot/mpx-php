{
  "name": "lullabot/mpx-php",
  "type": "library",
  "description": "A PHP 5.4+ library for accessing and interacting with data MPX services from ThePlatform (mpx.theplatform.com).",
  "keywords": [
    "MPX",
    "The Platform"
  ],
  "homepage": "http://bitbucket.com/lullabot/mpx-php",
  "license": "GPL-2.0+",
  "require": {
    "php": ">=5.6",
    "guzzlehttp/guzzle": "^6.0",
    "guzzlehttp/psr7": "^1.0",
    "psr/log": "^1.0",
    "cache/cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^2.9"
  },
  "autoload": {
    "psr-4": {
      "Mpx\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mpx\\Tests\\": "tests/src/"
    }
  }
}
