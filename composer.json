{
    "name": "lullabot/mpx-php",
    "type": "library",
    "description": "A PHP 5.4+ library for accessing and interacting with data MPX services from ThePlatform (mpx.theplatform.com).",
    "keywords": ["MPX", "The Platform"],
    "homepage": "http://bitbucket.com/lullabot/mpx-php",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/psr7": "~1.0",
        "pimple/pimple": "~3.0",
        "psr/log": "~1.0",
        "tedivm/stash": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*",
        "fabpot/php-cs-fixer": "1.9.*",
        "guzzlehttp/log-subscriber": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "Mpx\\": "src/"
        }
    }
}
