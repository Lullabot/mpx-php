{
  "name": "lullabot/mpx-php",
  "type": "library",
  "description": "A PHP library for accessing and interacting with data MPX services from ThePlatform (mpx.theplatform.com).",
  "keywords": [
    "MPX",
    "The Platform"
  ],
  "homepage": "http://bitbucket.com/lullabot/mpx-php",
  "license": "GPL-2.0+",
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^6.0",
    "guzzlehttp/psr7": "^1.0",
    "psr/log": "^1.0",
    "cache/cache": "^1.0",
    "symfony/serializer": "^3.4",
    "symfony/property-access": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "squizlabs/php_codesniffer": "^2.9",
    "friendsofphp/php-cs-fixer": "^2.10",
    "nette/php-generator": "^3.0",
    "symfony/console": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Lullabot\\Mpx\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lullabot\\Mpx\\Tests\\": "tests/src/",
      "Lullabot\\Mpx\\Command\\": "command/src/"
    }
  },
  "extra": {
    "composer-version": "^1.6.3"
  }
}
