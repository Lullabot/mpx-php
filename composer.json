{
  "name": "lullabot/mpx-php",
  "type": "library",
  "description": "A PHP library for accessing and interacting with data MPX services from ThePlatform (mpx.theplatform.com).",
  "keywords": [
    "MPX",
    "The Platform"
  ],
  "homepage": "http://bitbucket.com/lullabot/mpx-php",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": "^6.3",
    "guzzlehttp/psr7": "^1.0",
    "psr/log": "^1.0",
    "cache/cache": "^1.0",
    "symfony/serializer": "^3.4",
    "symfony/property-access": "^3.4",
    "doctrine/annotations": "^1.2",
    "symfony/property-info": "^3.4",
    "symfony/lock": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5||^7.0",
    "friendsofphp/php-cs-fixer": "2.15.1",
    "nette/php-generator": "^3.0",
    "symfony/console": "^3.4",
    "namshi/cuzzle": "^2.0",
    "rtheunissen/guzzle-log-middleware": "^0.4.1",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Lullabot\\Mpx\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lullabot\\Mpx\\Tests\\": "tests/src/",
      "Lullabot\\Mpx\\Command\\": "command/src/"
    }
  },
  "extra": {
    "composer-version": "^1.6.3"
  },
  "scripts": {
    "csFix": "vendor/bin/php-cs-fixer fix -v"
  }
}
